<div class="tech-slideshow">
  <div class="mover-1"></div>

</div>

<script type="text/javascript">
$(function(){
  let pixels = 0

  setInterval(function(){
    if(pixels >= 5000) {
      pixels = 0;
    } else {
      pixels += 1;
    }
      
    $(".mover-1").css("backgroundPosition", -pixels +"px 0%");
  }, 10)
})
  
  
</script>